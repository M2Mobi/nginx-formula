rewrite ^/?(.)*/(statics|build)/(.*)$ /$1/$2/$3 break;
rewrite ^/robots.txt$ /robots.txt break;
rewrite ^/favicon.ico$ /favicon.ico break;
rewrite ^/elb-status$ /elb-status break;
rewrite ^/.well-known/(.*)$ /.well-known/$1 last;
rewrite ^/?$ /index.php last;
rewrite ^/([a-zA-Z][a-zA-Z])/?$ /index.php?lang=$1 last;
rewrite ^/([a-zA-Z][a-zA-Z])/([^/]*)/?$ /index.php?lang=$1&controller=$2 last;
rewrite ^/([a-zA-Z][a-zA-Z])/([^/]*)/([^/]*)/?$ /index.php?lang=$1&controller=$2&method=$3 last;
rewrite ^/([a-zA-Z][a-zA-Z])/([^/]*)/([^/]*)/([^/]*)/?$ /index.php?lang=$1&controller=$2&method=$3&param1=$4 last;
rewrite ^/([a-zA-Z][a-zA-Z])/([^/]*)/([^/]*)/([^/]*)/([^/]*)/?$ /index.php?lang=$1&controller=$2&method=$3&param1=$4&param2=$5 last;
rewrite ^/([a-zA-Z][a-zA-Z])/([^/]*)/([^/]*)/([^/]*)/([^/]*)/([^/]*)/?$ /index.php?lang=$1&controller=$2&method=$3&param1=$4&param2=$5&param3=$6 last;
rewrite ^/([^/]*)/?(\?(.*))?$ /index.php?controller=$1&$3 last;
rewrite ^/([^/]*)/([^/]*)/?(\?(.*))?$ /index.php?controller=$1&method=$2&$4 last;
rewrite ^/([^/]*)/([^/]*)/([^/]*)/?(\?(.*))?$ /index.php?controller=$1&method=$2&param1=$3&$5 last;
rewrite ^/([^/]*)/([^/]*)/([^/]*)/([^/]*)/?(\?(.*))?$ /index.php?controller=$1&method=$2&param1=$3&param2=$4&$6 last;
rewrite ^/([^/]*)/([^/]*)/([^/]*)/([^/]*)/([^/]*)/?(\?(.*))?$ /index.php?controller=$1&method=$2&param1=$3&param2=$4&param3=$5&$7 last;
